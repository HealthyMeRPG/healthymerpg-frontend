<div class="container" id="trackers">
  <div class="row">
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <div class="panel-title">Fitbit</div>
        </div>
        <div class="panel-body">
          <img src="/assets/images/trackers/fitbit_logo_350_85.png" alt="Fitbit" class="center-block img-responsive" />
          {{#if hasFitbit}}
            <button class="btn btn-danger btn-block" {{action 'unregister' 'fitbit' on='click'}} id="unregister_fitbit">
              Unregister Tracker
            </button>
          {{else}}
            <button class="btn btn-primary btn-block" {{action 'register' 'fitbit' on='click'}} id="register_fitbit">
              Subscribe
            </button>
          {{/if}}
        </div>
      </div>
    </div>
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <div class="panel-title">HealthVault</div>
        </div>
        <div class="panel-body">
          <img src="/assets/images/trackers/healthvault_logo_221_38.png" alt="HealthVault" class="center-block img-responsive" />
          <button class="btn btn-primary btn-block" disabled="disabled">
            Coming Soon
          </button>
        </div>
      </div>
    </div>
  </div>
</div>
